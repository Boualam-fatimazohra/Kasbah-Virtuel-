{
    "buildCommand": "pnpm run vercel-build",
    "installCommand": "pnpm install --no-frozen-lockfile",
    "outputDirectory": "dist",
    "framework": null,
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/index.js"
      },
      {
        "handle": "filesystem"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }